<div class="row">
  <!-- Senators -->
  <div class="col p-2 border border-2 me-2">
    <h2>Senators</h2>
    <p>Below you will see all of the senators for <%=zipcode%></p>
    <div class="container text-center">
      <div class="row">
        <%// senators.forEach(()=>{%>
        <div class="col-12">
          <div class="card mb-2">
            <div class="row g-0">
              <div class="col-md-3 my-auto">
                <img
                  src="/img/girl.jpg"
                  class="img-fluid p-1 rounded"
                  alt="district map"
                />
              </div>
              <div class="col-md-7">
                <div class="card-body text-start">
                  <h5 class="card-title"><b>Debra Example</b> (D)</h5>
                  <p class="card-text">
                    <small><b>Website:</b></small>
                  </p>

                  <p class="card-text">
                    <small>Go to Profile</small>
                  </p>
                </div>
              </div>
              <div class="col-md-1 mx-auto">
                <div class="card-body text-start">
                  <a href="#">
                    <img
                      src="/img/heart_empty.png"
                      alt="heart"
                      width="20"
                      height="20"
                      onmouseover="hover(this)"
                      onmouseout="hover(this)"
                  /></a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-12">
          <div class="card mb-2">
            <div class="row g-0">
              <div class="col-md-3 my-auto">
                <img
                  src="/img/man.jpg"
